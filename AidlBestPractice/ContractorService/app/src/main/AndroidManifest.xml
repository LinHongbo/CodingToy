<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.sunniwell.contractor.service">

    <permission
        android:name="net.sunniwell.permission.CONTRACTOR"
        android:protectionLevel="normal" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <service android:name="net.sunniwell.contractor.ContractorService">
            <intent-filter>
                <action android:name="net.sunniwell.action.INTERFACE_SERVICE" />
            </intent-filter>
        </service>

    </application>

</manifest>